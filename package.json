{
  "name": "triple-taste-rpa",
  "version": "0.0.1",
  "description": "Triple Taste Munchies RPA",
  "scripts": {
    "prestart": "playwright install --with-deps chromium",
    "start": "cucumber-js -c cucumber.js",
    "preresults": "allure -q generate results/allure/ -c -o results/allure/html/",
    "results": "allure open results/allure/html/",
    "webhook-rpa": "cucumber-js -c cucumber.webhook.js",
    "webhook": "npx wrangler dev",
    "handler": "ts-node src/handler.ts",
    "report": "ts-node src/report.ts",
    "tunnel:webhook": "cloudflared tunnel --url http://localhost:8787",
    "tunnel:report": "cloudflared tunnel --url http://localhost:8686",
    "bootstrap": "ts-node src/bootstrap.ts",
    "serve:webhook": "pm2 start npm --name triple-taste-rpa-webhook -- run webhook",
    "serve:handler": "pm2 start npm --name triple-taste-rpa-handler -- run handler",
    "serve:report": "pm2 start npm --name triple-taste-rpa-report -- run report",
    "serve:tunnel:webhook": "pm2 start npm --name triple-taste-rpa-tunnel-webhook -- run tunnel:webhook",
    "serve:tunnel:report": "pm2 start npm --name triple-taste-rpa-tunnel-report -- run tunnel:report",
    "serve": "concurrently \"npm run serve:webhook\" \"npm run serve:handler\" \"npm run serve:report\" \"npm run serve:tunnel:webhook\" \"npm run serve:tunnel:report\"",
    "stop": "pm2 delete '/^triple-taste-rpa/'"
  },
  "keywords": [],
  "author": "iamkenos <alexis.matunog@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@iamkenos/kyoko": "^0.5.1",
    "@types/nodemailer": "^6.4.17",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fastest-levenshtein": "^1.0.16",
    "google-drive-getfilelist": "^1.0.5",
    "googleapis": "^142.0.0",
    "luxon": "^3.5.0",
    "nodemailer": "^6.9.16"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/luxon": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "wrangler": "^4.13.2"
  }
}
